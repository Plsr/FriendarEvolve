<%- include('header') %>

<div class="container-fluid">
  <div class="row">
    <div class="col-xs-12"><h1>Neues Event erstellen</h1></div>
  </div>
  <div class="row">
    <div class="col-xs-6">
      <form>
        <div class="form-group">
          <label for="eventNameInput">Eventname</label>
          <input type="email" class="form-control" id="eventNameInput" placeholder="Name">
        </div>
        <div class="form-group">
          <label for="ortInput">Ort</label>
          <input type="text" class="form-control" id="ortInput" placeholder="Ort">
        </div>
        <div class="form-group">
          <label for="dateInput">Datum</label>
          <input type="date" class="form-control" id="dateInput" placeholder="DD/MM/YYYY">
        </div>
        <div class="form-group">
          <label for="timeInput">Zeit</label>
          <input type="time" class="form-control" id="timeInput" placeholder="DD/MM/YYYY">
        </div>
        <button type="submit" class="btn btn-default">Submit</button>
      </form>
    </div>
  </div>
</div>

<%- include('footer') %>